enclave{
    // 方向属性只能加在strcut上，无法加载member上
     include "types.h"
     struct test{
         [in, out, count = arr_len]int * arr;
         size_t arr_len;
    };
     struct ecall_layer{
         size_t batch;
         size_t inputs;
         size_t outputs;
         size_t h;
         size_t w;
         size_t c;
         size_t out_h;
         size_t out_w;
         size_t out_c;

         int batch_normalize;
         ACTIVATION a;

         size_t input_len;
         [count=input_len]       float* input;
        
         size_t output_len;
