         size_t weights_len;
         [count=weight_len]      float* weights;
        
         size_t bias_len;
         [count=bias_len]        float* biases;
        
         size_t rolling_len;
         [count=rolling_len]    float* rolling_mean;
         [count=rolling_len]    float* rolling_variance;

         size_t x_len;
         [count=x_len]      float* x;
         [count=x_len]      float* x_norm;
         };
};